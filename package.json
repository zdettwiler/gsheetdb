{
  "name": "gsheetdb",
  "version": "1.0.2",
  "description": "Use Google Sheets as a simple database.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec sh -ac 'source .env && babel-node ./example.js'",
    "test": "jest --watch",
    "compile": "babel src --out-dir lib",
    "prepare": "npm run compile"
  },
  "keywords": [
    "google",
    "sheets",
    "spreadsheet",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zdettwiler/gsheetsdb.git"
  },
  "author": "Zach Dettwiler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zdettwiler/gsheetsdb/issues"
  },
  "homepage": "https://github.com/zdettwiler/gsheetsdb",
  "dependencies": {
    "google-auth-library": "^4.2.6",
    "googleapis": "^39.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-jest": "^25.5.1",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
